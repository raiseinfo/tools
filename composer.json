{
  "name": "raiseinfo/tools",
  "description": "A simple tool powered by raiseinfo.",
  "type": "library",
  "license": "MIT",
  "require": {
    "php": "^7.4 || ^8.0"
  },
  "autoload": {
    "psr-4": {
      "Raiseinfo\\Tools\\": "src/"
    }
  },
  "authors": [
    {
      "name": "raiseinfo",
      "email": "you@example.com"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require-dev": {
    "phpunit/phpunit": "^11.5"
  },
  "autoload-dev": {
    "psr-4": {
      "Raiseinfo\\Tools\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit"
  }
}
